{# Purpose of this template: Display item information for previewing from other modules #}
<dl id="location{{ location.getKey() }}">
<dt>{{ location|musharemodule_formattedTitle|notifyFilters('share.filter_hooks.locations.filter')|safeHtml }}</dt>
{% if location.description is not empty %}<dd>{{ location.description }}</dd>{% endif %}
</dl>
