{# Purpose of this template: Display messages within an external context #}
{% for message in items %}
    <h3>{{ message|musharemodule_formattedTitle }}</h3>
    <p><a href="{{ path('musharemodule_message_display', {'id': message.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
