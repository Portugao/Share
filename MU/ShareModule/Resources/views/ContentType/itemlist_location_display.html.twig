{# Purpose of this template: Display locations within an external context #}
{% for location in items %}
    <h3>{{ location|musharemodule_formattedTitle }}</h3>
    <p><a href="{{ path('musharemodule_location_display', {'id': location.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
