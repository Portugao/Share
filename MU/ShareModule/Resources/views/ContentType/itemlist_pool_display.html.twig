{# Purpose of this template: Display pools within an external context #}
{% for pool in items %}
    <h3>{{ pool|musharemodule_formattedTitle }}</h3>
    <p><a href="{{ path('musharemodule_pool_display', {'id': pool.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
